<h1>Admins::Teams#index</h1>

<p><li><%= link_to "Add Team", new_admins_team_path %></li></p>

<% @teams.each do |team| %>
  <p>
    <b><%= team.name %></b>
    <%= link_to "Show", admins_team_path(team) %>
    <%= link_to "Edit", edit_admins_team_path(team) %>
    <%= link_to "Delete", admins_team_path(team), method: :delete %>
  </p>
<%end%>
